        },
        {
          "content": "Prayers may be offered for ourselves and others.",
          "line_type": "rubric",
          "indented": false,
          "preface": null,
          "extra_space_before": false,
          "audio_heading_content": null,
          "audio_id": "379a8df2-b279-5902-81af-2678a22288e9",
